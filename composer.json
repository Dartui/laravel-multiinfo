{
    "name": "dartui/multiinfo",
    "description": "Polkomtel MultiInfo integration for sending SMS from Laravel applications ",
    "keywords": ["polkomtel", "multiinfo", "laravel", "sms"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Krzysztof Grabania",
            "email": "krzysztof.grabania@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "nesbot/carbon": "^1.0 || ^2.0",
        "illuminate/config": "^5.4 || ^6.0",
        "illuminate/contracts": "^5.4 || ^6.0",
        "illuminate/support": "^5.4 || ^6.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Dartui\\Multiinfo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dartui\\Multiinfo\\ServiceProvider"
            ]
        }
    }
}
